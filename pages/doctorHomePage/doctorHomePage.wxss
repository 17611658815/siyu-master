/* tab切换 */
.list{
  width: 100%;
  height:130rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  background: #ffffff;
  box-shadow: 0 10rpx 20rpx #eee;
  position: sticky;
  position: -webkit-sticky;
  top: 0;
}
.list>view{
  width: 90%;
  height:120rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  background: #ffffff;
}
.list text{
  padding-bottom: 18rpx;
}
