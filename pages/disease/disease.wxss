/* pages/disease/disease.wxss */
.page-body {
  display: flex;
  flex: 1;
  flex-direction: column;
  margin-bottom: 98rpx;
  /* background: #f8f8f8; */
}
.box{
  width: 100%;
  margin-top: 20rpx;
}
.header{
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
  margin-bottom: 40rpx;
}
.header>view:nth-child(1){
  width: 100%;
  height: 130rpx;
  background: linear-gradient(#6EAFF7, #6E9FF7);
  display: flex;
  align-items: center;
}
.header image{
  width: 100rpx;
  height: 92%;
  margin-left: 36rpx;
  margin-right: 40rpx;
  padding-top: 13rpx
}
.header>view:nth-child(1)>text{
  color: #FFFFFF;
  font-size: 40rpx;
  font-weight: 500;
}
.list{
  width: 100%;
  height:130rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  background: #ffffff;
 box-shadow: 0 10rpx 20rpx #eee;
}
.list>view{
  width: 92%;
  height:120rpx;
  margin: auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  background: #ffffff;
}
.list text{
  padding-bottom: 18rpx;
}
/* tab切换样式 */
 .swiper-tab-list {
  font-size: 32rpx;
  display: inline-block;
  position: relative;
} 
.on{
  color: #53a2ef;
  /* border-bottom: 6rpx solid #53a2ef; */
  font-size: 36rpx;
  font-weight: 600;
} 

.solid{
  width: 100%;
  background: #53a2ef;
  height: 6rpx;
  /* border: 2rpx solid #53a2ef; */
  border-radius: 40rpx 40rpx 40rpx 40rpx;
  position: absolute;
  left: 0;
  top: 60rpx;


}
/* 折叠展开 */
.textMsg{
  width: 100%;
  align-items: center;
  justify-content: center;
  background: #ffffff;
  margin-top: 20rpx;
}
.textMsg .folding{
  width: 92%;
  margin: auto;
  font-size: 28rpx;
  padding-bottom: 27rpx;
  border-bottom: 1rpx solid #e2e2e2;
}
.folding text{
  line-height: 50rpx;
}

.textMsg .folding>text:nth-child(2){
  color: #53a2ef
}
/* 就医指南 */
.guide{
  width: 100%;
  background: #ffffff;
}
.title{
  width: 100%;
  margin: auto;
  display: flex;
  position: relative;
  align-items: center;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
}
.title image{
  width: 30rpx;
  height: 30rpx;
}

.title text:nth-child(2){
  font-size: 32rpx;
  color: #000000;
  margin-left: 20rpx;
  font-weight: 500;
}
.title text:nth-child(3){
  font-size: 26rpx;
  color: #999999;
  position: absolute;
  right: 0;
}
.guide_Msg{
  width:100%;
  margin: auto;
  display: flex;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.guide_Msg text:nth-child(1){
  white-space: nowrap;
}
.guide_Msg text:nth-child(2){
  margin: 0 10rpx;

}
.guide_Msg text:nth-child(3){
    overflow: hidden;
text-overflow: ellipsis;
white-space: nowrap;

  color: #909999;
  
}
.unfold{
  margin-left: 20rpx;
  color: #53a2ef
}

.fix-news{
  position: fixed;
  top:0;
  left: 0;
  z-index: 99999;
}
.content{
  width: 92%;
  margin: auto;
  /* border-bottom: 1rpx solid #e2e2e2; */
  padding-bottom: 30rpx; 
}
/* 热门视频文章列表 */
.hotList{
   background: #ffffff;
}
.hotListBox{
  width: 92%;
  margin: auto;
}
/* 推荐列表样式 */
.msgList_box{
  width: 92%;
  margin: auto;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #e2e2e2;
}
.msgList_title{
  width: 100%;
 
  display: flex;
}
.msgList_title .video{
  width: 50rpx;
  height: 35rpx;
  border: 1rpx solid #4cd093;
  font-size: 22rpx;
  text-align: center;
  line-height: 35rpx;
  color:#4cd093;
  padding: 0 8rpx;
  border-radius: 8rpx;
  margin-right: 20rpx;
}
.msgList_title .question{
  width: 50rpx;
  height: 35rpx;
  border: 1rpx solid #ff952d;
  font-size: 22rpx;
  text-align: center;
  line-height: 35rpx;
  color:#ff952d;
  padding: 0 8rpx;
  border-radius: 8rpx;
  margin-right: 20rpx;
}
.msgList_title .audio{
  width: 50rpx;
  height: 35rpx;
  border: 1rpx solid #4c9dee;
  font-size: 22rpx;
  text-align: center;
  line-height: 35rpx;
  color:#4c9dee;
  padding: 0 8rpx;
  border-radius: 8rpx;
  margin-right: 20rpx;
}
.msgList_title .problem{
 font-size: 32rpx;
}
.msgList_msg{
  /* display: flex; */
  margin-top: 10rpx;
  /* align-items: center; */
}
/* 问答样式 */
.msgList_answer{
  display: flex;
  margin-top: 25rpx;
}
.msgList_answer .articles{
  flex: 1;
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #666666;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 2; /** 显示的行数 **/
  overflow: hidden;  /** 隐藏超出的内容 **/
  line-height: 44rpx;
}
.msgList_answer .article{
  flex: 1;
  font-size: 28rpx;
  color: #666666;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 2; /** 显示的行数 **/
  overflow: hidden;  /** 隐藏超出的内容 **/
  line-height: 44rpx;
}
.msgList_answer image{
  width: 30rpx;
  height: 35rpx;
  margin-top: 10rpx;
}
.msgList_title image{
  width: 30rpx;
  height: 35rpx;
  padding-top: 7rpx;
  margin-right: 20rpx;
}
