/* 音频样式 */
.msgList_audio{
  display: flex;
  margin-top: 30rpx;
  align-items: center;
}
.msgList_audio .answerlogo{
  width: 32rpx;
  height: 35rpx;
}
.msgList_audio .audio_time{
  width: 424rpx;
  height: 64rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #f3f3f3;
  border-radius: 8rpx;
  margin-left: 20rpx;
}
.audio_time image{
  width: 32rpx;
  height: 32rpx;
}
.audio_time .timertext{
  font-size: 24rpx;
  color: #333333;
  margin-left: 38rpx; 
}
.audio_time .timer{
  font-size: 24rpx;
  color: #f19536;
  margin-left: 38rpx; 
}
.msgList_audio .audiohot{
  display: flex;
  font-size: 22rpx;
  color: #999999;
  margin-left: 20rpx;
}
.msgList_audio .audiohot image{
  width: 40rpx;
  height: 26rpx;
  margin-right: 5rpx;
}

