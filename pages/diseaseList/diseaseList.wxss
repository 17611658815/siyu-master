.container{
  width: 100%;
  height: 100%;
  background: #f8f8f8;
  border-top: 1rpx solid #e2e2e2;
}
/* 搜索框 */
.searchBox{
  width: 100%;
  /* height: 100rpx; */
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  top: 20rpx;
  background: #ffffff;
  
}
.search{
  width: 690rpx;
  margin: auto;
  height: 60rpx;
  border-radius: 30rpx;
  background: #f8f8f8;
}
.searchInput{
  width: 550rpx;
  height: 60rpx;
  color: #333333;
  padding: 0;
  font-size: 30rpx;
  line-height: 60rpx;
  margin-left: 50rpx;
  border: none;
  background: #f8f8f8;
}
.phcolor{
  color: #999999;
}
.lineShu{
  height: 30rpx;
  width: 2rpx;
  background: #999999;
  margin-top: 18rpx;
}
.searchIcon{
  width: 32rpx;
  height: 33rpx;
  margin-top: 16rpx;
  margin-right: 28rpx;
}
/*diseases*/
.diseaseList{
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  position: fixed;
  top: 80rpx;
  left: 0;
}
.menu{
  width:100%;
  height: 100%;
  font-size:30rpx;
  color: #666666;
  /* overflow-y: auto; */
}
.menu-item{
  width: 90%;
  margin: auto;
  line-height: 100rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* text-indent: 10rpx; */
  /* background: #f8f8f8; */
  position: relative;
  color: #333333;
}
.menu-item.selected{
  /* background: #6ea8f7; */
  color:#6ea8f7;
  font-weight: 500;
  font-size: 30rpx;
}
.menu-item:last-child{
  border-bottom: none;
}
.menu-detail-item{
  width: 90%;
  height: auto;
  margin: auto;
  background: #ffffff;
  display: flex;
  flex-wrap: wrap;
  font-size: 28rpx;
  /* justify-content: space-between; */
  padding-bottom: 40rpx;
}
.text-box{
  display: flex;
  width: 33%;
  font-size: 28rpx;
  color: #666666;
  margin-top: 40rpx;
  /* padding-left: 20rpx; */
  /* text-indent:10px */
}
/* .menu-detail-item text:nth-child(1){
  text-indent:0px;
}
.menu-detail-item text:nth-child(4){
  text-indent:0px;
}
.menu-detail-item text:nth-child(7){
  text-indent:0px;
} */
.menuArrow{
  width: 30rpx;
  height: 18rpx;
}
.menu-detail{
  border-bottom: 20rpx solid rgba(144,144,144,0.1);
}
.hide{
  display: none;
}
.show{
  display: block;
}
.active-tag{
  color: #6ea8f7;
}
.small-title{
  text-indent: 60rpx;
  color: #333333;
}
.small-title-active{
  color: #6ea8f7;
}
.diseases{
  flex: 1;
  width: 100%;
  height: 100%;
  overflow-y:auto; 
  position: relative;
  /* white-space: nowrap;  */
  text-overflow: ellipsis;
}

.active{
  color: #6ea8f7;
}
.box{
  display: inline-block;
  padding: 10rpx 20rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  /* border: 1rpx solid #e2e2e2; */
  color: #666666;
  font-size: 28rpx;
}
.menu-title{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
 
}
.blocks{
  width: 100%;
  /* border-bottom: 1rpx solid #e2e2e2; */
}
.menu-item-box{
  width: 100%;
  border-bottom: 1rpx solid #e2e2e2;
}
.son_name{
  font-size: 28rpx;
  color: #666666;
}
