/*模态窗口*/
.modal{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0,0.6)
}
.dialogue{
  width: 564rpx;
  height: 440rpx;
  border-radius: 16rpx;
  margin: 198rpx auto 0;
  padding-top: 84rpx;
  background-color: #ffffff;
  position: relative;
}
.close{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 30rpx;
  right: 30rpx;
}
.diaPicBox{
  width: 100%;
  height: 160rpx;
}
.diaPic{
  width: 140rpx;
  height: 140rpx;
  margin: 0 auto;
}
.prompt{
  width: 100%;
  text-align: center;
  margin-top: 10rpx;
}
.proTitle{
  font-size: 46rpx;
  color: #3c3d41;
  font-weight: 600;
  line-height: 70rpx;
}
.time{
  color: #898989;
  font-size:24rpx;
  line-height: 30rpx;
  margin-top: 10rpx
}
.button{
  width: 500rpx;
  height: 94rpx;
  margin: 44rpx auto 0;
}
.check{
  font-size: 36rpx;
  color: #ffffff;
  background-color: #FF0036;
}
/* 问答样式 */
.msgList{
  width: 100%;
  background: #ffffff;
  border-top:20rpx solid rgba(144,144,144,0.1);
}


.msgList_box{
  width: 92%;
  margin: auto;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #e2e2e2;
}
.msgList_answer{
  display: flex;
  margin-top: 25rpx;
}
.msgList_answer .articles{
  flex: 1;
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #666666;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 2; /** 显示的行数 **/
  overflow: hidden;  /** 隐藏超出的内容 **/
  line-height: 44rpx;
}
.msgList_answer .article{
  flex: 1;
  font-size: 28rpx;
  color: #666666;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 2; /** 显示的行数 **/
  overflow: hidden;  /** 隐藏超出的内容 **/
  line-height: 44rpx;
}
.msgList_answer image{
  width: 30rpx;
  height: 35rpx;
  margin-top: 10rpx;
}
.msgList_title image{
  width: 30rpx;
  height: 35rpx;
  margin-right: 20rpx;
}
.msgList_title{
  width: 100%;
  display: flex;
  align-items: center;
}
.msgList_title .problem{
  font-size: 32rpx;
}
